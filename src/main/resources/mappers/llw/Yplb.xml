<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gson.his.model.mappers.llw.YplbMapper">

    <select id="lbcx" resultType="cn.gson.his.model.pojos.llw.LbEntity">
        select * from lb
        <where>
            <if test="lbName!=null and lbName!=''">
                and lb_name like '%${lbName}%'
            </if>
        </where>
    </select>

    <select id="ypcx" resultType="cn.gson.his.model.pojos.llw.DrugEntity">
        select * from drug where lb_id=#{id}
    </select>
</mapper>